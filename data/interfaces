# This file describes the network interfaces available on your system
# and how to activate them. For more information, see interfaces(5).

# The loopback network interface
auto lo
iface lo inet loopback

# The primary network interface
auto em1
iface em1 inet static
	address 149.154.68.48
	netmask 255.255.254.0
	network 149.154.68.0
	broadcast 149.154.69.255
	gateway 149.154.69.254
	# dns-* options are implemented by the resolvconf package, if installed
	dns-nameservers 188.120.247.2
	dns-search rial.cc

auto em1:0
iface em1:0 inet static
    address 94.250.254.176
    netmask 255.255.254.0
    nerwork 94.250.254.0
    broadcast 94.250.255.255
    gateway 94.250.255.254
    dns-nameservers 188.120.247.2
    dns-search rial.cc

auto em1:1
